<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.trafficmi">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.TrafficMi">
        <activity android:name=".ViewVehicleTheft"></activity>
        <activity android:name=".DriverOffenseDetail" />
        <activity android:name=".UpdatedCarRecords" />
        <activity android:name=".ReportVehicleTheft" />
        <activity android:name=".UpdateVehicleRecords" />
        <activity android:name=".UpdateDriverRecords" />
        <activity android:name=".ChiefTrafficOfficerChoiceOfWork" />
        <activity android:name=".ChiefTrafficLogIn" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>